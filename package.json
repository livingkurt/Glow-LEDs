{
  "name": "glow_leds",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "dependencies": {
    "@easypost/api": "^7.3.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "form-data": "^4.0.1",
    "googleapis": "^103.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "if-env": "^1.0.4",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^6.7.0",
    "mongoose": "^7.6.12",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "qrcode": "^1.5.4",
    "request": "^2.88.2",
    "sales-tax": "^2.13.1",
    "sass": "^1.77.3",
    "socket.io": "^4.7.5",
    "stripe": "^13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/core": "^7.24.6",
    "@babel/node": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jest-enzyme": "^7.1.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.34.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^3.4.1"
  },
  "scripts": {
    "start": "npm run env-check && npm run start:prod || npm run start:dev",
    "env-check": "cross-env NODE_ENV=production",
    "start:prod": "node dist/server.js",
    "start:dev": "concurrently \"npm:server-dev\" \"npm:client\" \"npm:serve:client\"",
    "transpile-watch": "babel server --out-dir dist --ignore node_modules --watch",
    "server-dev": "nodemon dist/server.js",
    "serve:client": "cd client && npm run preview",
    "client": "cd client && npm run dev -- --host",
    "build": "babel server --out-dir dist --source-maps --copy-files",
    "heroku-postbuild": "npm run build && cd client && npm install --legacy-peer-deps && npm run build",
    "install": "cd client && npm install --legacy-peer-deps",
    "build:client": "cd client && npm run build && npm run preview",
    "clean-install": "rm -rf node_modules package-lock.json client/node_modules client/package-lock.json && npm install && cd client && npm install --legacy-peer-deps && cd ..",
    "test": "jest",
    "test:all": "npm --test-all",
    "test:backend": "npm test --config=jest.config.js",
    "cypress:run": "cd client && npm run cypress:run",
    "g": "node server/scripts/generateMigration.js",
    "lint": "node --experimental-vm-modules node_modules/eslint/bin/eslint.js server",
    "lint:fix": "node --experimental-vm-modules node_modules/eslint/bin/eslint.js server --fix",
    "prettier": "prettier --check . && cd client && prettier --check .",
    "prettier:fix": "prettier --write . && cd client && prettier --write .",
    "generate-migration": "node server/api/migrations/scripts/migration_generator.js",
    "migrate": "node -e \"import('./server/api/migrations/scripts/migration_runner.js').then(m => m.runMigrations('up'))\"",
    "migrate:down": "node server/api/migrations/scripts/migration_runner.js down",
    "migrate:redo": "node server/api/migrations/scripts/migration_runner.js redo"
  },
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "client/src/**/*.jsx": [
      "eslint --fix --cache"
    ]
  }
}
