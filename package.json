    "@babel/core": "^7.24.6",
    "@babel/node": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@bugsnag/js": "^7.23.0",
    "@bugsnag/plugin-express": "^7.22.7",
    "@easypost/api": "^7.3.0",
    "app-root-path": "^3.1.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "googleapis": "^103.0.0",
    "heroku-ssl-redirect": "^0.1.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jimp": "^0.22.10",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "mongodb": "^6.7.0",
    "mongoose": "^7.6.12",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "papaparse": "^5.4.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pdf2pic": "^2.2.1",
    "request": "^2.88.2",
    "sales-tax": "^2.13.1",
    "sass": "^1.77.3",
    "socket.io": "^4.7.5",
    "stripe": "^13.3.0"
  },
  "devDependencies": {
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jest-enzyme": "^7.1.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.1.2",
    "pre-commit": "^1.2.2",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^5.1.0"
  },
  "scripts": {
    "start": "npm run env-check && npm run start:prod || npm run start:dev",
    "env-check": "cross-env NODE_ENV=production",
    "start:prod": "node --max_old_space_size=2560 dist/server.js",
    "start:dev": "concurrently \"npm:server-dev\" \"npm:client\"",
    "transpile-watch": "babel server --out-dir dist --ignore node_modules --watch",
    "server-dev": "nodemon --max_old_space_size=2560 dist/server.js",
    "client": "cd client && npm start",
    "build": "babel server --out-dir dist",
    "heroku-postbuild": "npm run build && npm run build:client",
    "install": "cd client && npm install",
    "build:client": "cd client && npm run build",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint 'client/src/**/*.{js,jsx}' --cache",
    "lint:scss": "stylelint 'client/src/**/*.scss' --cache",
    "sitemap": "cd client && npm run generate-sitemap",
    "lint-staged": "lint-staged",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "lint:js:fix": "npm run lint:js --fix",
    "lint:scss:fix": "npm run lint:scss --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "clean-install": "rm -rf node_modules package-lock.json client/node_modules client/package-lock.json && npm install && cd client && npm install && cd ..",
    "test": "jest",
    "test:all": "npm --test-all",
    "test:backend": "npm test --config=jest.config.js",
    "cypress:run": "cd client && npm run cypress:run",
    "g": "node server/scripts/generateMigration.js"
  },
  "engines": {
    "node": "18.20.1",
    "npm": "10.2.4"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "client/src/**/*.jsx": [
      "eslint --fix --cache"
    ],
    "client/src/**/*.scss": [
      "stylelint --fix --cache"
    ]
  }
}
