  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "tsc": "tsc --outDir dist",
    "watch": "tsc -w --outDir dist",
    "postinstall": "npm run tsc",
    "start:prod": "node dist/server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" \"npm run watch\" ",
    "client": "cd client && npm run start",
    "sitemap": "cd client && npm run sitemap",
    "install": "cd client && npm install",
    "build": "tsc && npm run build:client",
    "build:client": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "lint": "npm lint:scss && npm lint:js",
    "lint-staged": "$(npm bin)/lint-staged",
    "lint:fix": "npm lint:js:fix && npm lint:scss:fix",
    "lint:js": "eslint client/src --ext jsx,js --cache ",
    "lint:js:fix": "npm lint:js --fix",
    "lint:scss": "stylelint './client/src/scss/**/*.scss' --cache",
    "lint:scss:fix": "npm lint:scss --fix",
    "stylelint-check": "stylelint-config-prettier-check"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "client/src/**/*.jsx": [
      "eslint --fix --cache"
    ],
    "client/src/scss/**/*.scss": [
      "stylelint --fix --cache"
    ]
  }
}
