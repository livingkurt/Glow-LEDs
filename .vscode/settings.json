{
  "workbench.colorTheme": "One Dark Pro",
  "editor.tabSize": 2,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.iconTheme": "vscode-icons",
  "[graphql]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "terminal.integrated.fontFamily": "Source Code Pro for Powerline",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.cursorStyle": "line",
  "emmet.includeLanguages": {
    "django-html": "html"
  },
  "files.associations": {
    "*.ts": "typescript",
    "*.js": "javascript"
  },
  "[python]": {
    "editor.defaultFormatter": "geecode.geecode-python"
  },
  "eslint.enable": true,
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.run": "onType",
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "eslint.lintTask.enable": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "cSpell.words": [
    "Arrazati",
    "Capez",
    "clozd",
    "glowframez",
    "glowskinz",
    "glowstringz",
    "lilwhip",
    "Nanoskinz",
    "Novaskinz",
    "Omniskinz",
    "opyn",
    "Qoagv",
    "uuidv"
  ],
  "atlascode.jira.explorer.nestSubtasks": false,
  "atlascode.jira.explorer.enabled": true,
  "atlascode.jira.hover.enabled": true,
  "atlascode.bitbucket.enabled": false,
  "atlascode.jira.enabled": true,
  "atlascode.jira.jqlList": [
